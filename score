score = input("Enter Score: ")

try:
    score = float(score)  # Convert the input to a floating-point number

    if score < 0.6:
        print('D')
    elif score >= 0.9:
        print('A')
    elif score >= 0.8:
        print('B')
    elif score >= 0.7:
        print('C')
    elif score >= 0.6:  # This should be >= instead of just >
        print('F')
    else:
        print('Error')
except ValueError:
    print("Please enter a valid numeric score.")

